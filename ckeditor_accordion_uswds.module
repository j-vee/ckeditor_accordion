<?php
/**
 * @file
 * Functions to support creating CKEditor Accordion USWDS.
 */

/**
 * Implements hook_page_attachments_alter().
 */
function ckeditor_accordion_page_attachments_alter(array &$page) {
  // Attaches css assets globally.
  $page['#attached']['library'][] = 'drupal_ckeditor_accordion_uswds/accordion_style';
}
